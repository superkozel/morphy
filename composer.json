{
    "name": "superkozel/morphy",
    "description": "morpher.ru webservice PHP API library",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Dmitriy Zykov",
            "email": "aesleep@ya.ru"
        }
    ],
    "support": {
        "issues":     "https://github.com/superkozel/morphy/issues",
        "source":     "https://github.com/superkozel/morphy"
    },
    "require": {
        "php": "^7.3|^8.0",
        "curl/curl": "^2.2"
    },
    "autoload": {
        "psr-4": {
            "Morphy\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Morphy\\MorphyServiceProvider"
            ],
            "aliases": {
                "Morphy": "Morphy\\Morphy"
            }
        }
    },
    "require-dev": {
        "laravel/framework": "^5.8"
    }
}
